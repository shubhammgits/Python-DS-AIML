   "source": [
    "#Sequence Sum: 1/1! + 2/2! + 3/3! +.....+N/N!\n",
    "\n",
    "\n",
    "n = int(input(\"Enter the Nth Number : \"))\n",
    "fact, sum = 1,0\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    fact = fact*i\n",
    "    seq = i/fact\n",
    "    sum = sum + seq\n",
    " \n",
    "print(sum)"
   ]
